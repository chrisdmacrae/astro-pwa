---
import { CollectionEntry, getCollection } from "astro:content"
import SpaLayout from "../../layouts/SpaLayout.astro"

export interface Props {
  slugs: CollectionEntry<'pages'>['slug'][]
}

export const getStaticPaths = async () => {
  const blogs = await getCollection('pages')

  return blogs.map(blog => ({ params: { slug: blog.slug }, props: { slugs: blogs.map(blog => blog.slug )}}))
}
---
<SpaLayout title="Dynamic Route" />