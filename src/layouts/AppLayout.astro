---
import App from "../components/react/App";
import type { Store } from "../lib/store";
import SpaLayout from "./SpaLayout.astro";

export type Props = {
  title: string
  stores?: Store[]
}

const { title, stores } = Astro.props
---
<SpaLayout title={title} stores={stores}>
  <App client:load>
    <slot />
  </App>
</SpaLayout>