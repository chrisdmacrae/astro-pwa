---
import ServerSPALayout from "../../../layouts/ServerSPALayout.astro";
import { Router } from "../../../components/react/Router";

export const routes = ['c', 'd']

export const prerender = true
---
<ServerSPALayout title="Routing Root" filePath={Astro.self.moduleId!.replace(process.cwd(), `https://github.com/chrisdmacrae/astro-pwa/tree/main/packages/examples`)}>
  <Router baseUrl="/server-spa" routes={routes} client:load />
</ServerSPALayout>
