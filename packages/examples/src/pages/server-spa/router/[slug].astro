---
import ServerSPALayout from "../../../layouts/ServerSPALayout.astro";
import { Router, routes } from "../../../components/react/Router";

const { slug } = Astro.params
---
<ServerSPALayout title={`Dynamic Route: ${slug}`} filePath={Astro.self.moduleId!.replace(process.cwd(), `https://github.com/chrisdmacrae/astro-pwa/tree/main/packages`)}>
  <Router baseUrl="/examples/server-spa" routes={routes} client:load />
</ServerSPALayout>
