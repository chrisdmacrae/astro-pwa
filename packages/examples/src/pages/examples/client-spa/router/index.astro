---
import ClientSPALayout from "../../../../layouts/ClientSPALayout.astro";
import { Router, routes } from "../../../../components/react/Router";

export const prerender = true
---
<ClientSPALayout title="Routing Root" filePath={Astro.self.moduleId!.replace(process.cwd(), `https://github.com/chrisdmacrae/astro-pwa/tree/main/packages/examples`)}>
  <Router baseUrl="/examples/client-spa" routes={routes} client:only="react" />
</ClientSPALayout>
