{
    "rewrites": [
      {
        "source": "/examples/",
        "destination": "https://astro-pwa-demo.vercel.app/examples"
      },
      {
        "source": "/examples/:path*",
        "destination": "https://astro-pwa-demo.vercel.app/examples/:path*"
      },
      {
        "source": "/docs",
        "destination": "https://astro-pwa-docs.vercel.app/docs"
      },
      {
        "source": "/docs/:path*",
        "destination": "https://astro-pwa-docs.vercel.app/docs/:path*"
      }
    ]
  }
  